    "def elements(listis): \n",
    "    n = 0\n",
    "    for num in listis: \n",
    "        if isinstance(num, tuple): \n",
    "            break\n",
    "        n = n + 1\n",
    "    return n \n",
    "listis = [5, 8, 0, (2,4), 99, (8,7), 3] \n",
    "print(\"The elements in a list until an element is a tuple are:\")\n",
    "print(elements(listis))"
   ]
